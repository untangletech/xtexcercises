<!--[if IE 9]><html lang="en" class="ie9"><![endif]-->
<!--[if gt IE 9]><!--><html lang="en"><!--<![endif]-->
<head>
    {{> head }}
</head>
<body>
    <div class="page-wrap">
        {{> header }}
        <div id="mobile-wrap">
            <div id="content" class="content">
                <div class="container">
                    <div class="row main">
                        <div class="col-sm-8" role="main">
                            {% body %}
                        </div>
                        <div class="sidebar col-sm-4" role="complementary">
                            {{> right }}
                        </div>
                    </div>
                </div>
            </div>
            {{> footer }}
        </div>
    </div>
    {{> scripts}}
</body>
</html>