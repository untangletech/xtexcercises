{{!GRID}}
<div class="row mb-5">
    <a class="col-12 col-md-4 grid-info" href ="aboutUs.html">
        <img class="d-block grid-img" src="{{assets}}/images/Grid1.png" alt="Our Story Image">
        <div class="overlay-gray-block"></div>
        <h5 class="overlay-center">OUR BRAND STOY</h5>
    </a>
    <a class="col-12 col-md-4 grid-info" href ="collections.html">
        <img class="d-block grid-img" src="{{assets}}/images/Grid2.jpg" alt="Alhambra Collection">
        <div class="overlay-gray-block"></div>
        <h5 class="overlay-center">ALHAMBRA COLLECTION</h5>
    </a>
    <a class="col-12 col-md-4 grid-info" href ="collections.html">
        <img class="d-block grid-img" src="{{assets}}/images/Grid3.jpg" alt="Christmas Collection">
        <div class="overlay-gray-block"></div>
        <h5 class="overlay-center"> CHRISTMAS COLLECTION</h5>
    </a>
</div>
<div class="message text-center mt-5 mb-5">
    <h5 class="semi-important-font bold mb-5">Featured Products</h5>
    <img class="line" src="{{assets}}/images/Line.png"/>
</div>
{{!CARDS}}
    <div class="card-section" id="homeCart">
<div class="card-columns cards-desktop mb-5">
      {{#eachUpTo (getDataArray site.categories.0.category site.catalog) 5}}
  <div class="card">
    <img class="card-img-top" src="../../assets/images/{{this.img}}" alt="Product 3">
    <div class="card-body">
      <h5 class="card-title mb-0">{{this.name}}</h5>
      <p class="card-text mb-0">${{this.price.regular}}.00</p>
      <div class="d-inline-block">
          <span aria-hidden="true" class="icon-star"></span>
          <span aria-hidden="true" class="icon-star"></span>
          <span aria-hidden="true" class="icon-star"></span>
          <span aria-hidden="true" class="icon-star"></span>
          <span aria-hidden="true" class="icon-star-outline"></span>
      </div>
      <div class="d-inline-block mt-1">
        <a class="no-link-style" href="cart.html">
      <button class="btn card-btn-pdp add-to-cart">Add to Cart</button>
      </a>
      <span aria-hidden="true" class="icon-wishlist m-1 pt-1"></span>
      <span aria-hidden="true" class="icon-compare"></span>
      </div>
    </div>
  </div>
     {{/eachUpTo}}
</div>
</div>
